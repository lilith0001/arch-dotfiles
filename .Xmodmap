clear Lock
clear Control
clear mod4

keysym Caps_Lock = Control_L
keysym Control_L = Caps_Lock
add Lock = Caps_Lock
add Control = Control_L

keycode 117 = Super_L
keycode 108 = Mode_switch
keycode 24  = q Q Home Q
keycode 25  = w W Up W
keycode 26  = e E End E
keycode 38  = a A Left A
keycode 39  = s S Down S
keycode 40  = d D Right D
